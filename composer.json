{
  "name": "aronpc/youtube-comments",
  "description": "A PHP application to download YouTube comments without using API keys",
  "type": "project",
  "require": {
    "php": ">=8.0",
    "symfony/console": "^7.2",
    "symfony/process": "^7.2.5",
    "illuminate/support": "^12.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "YouTube Comments Dump",
      "email": "info@example.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "test": "phpunit"
  }
}
